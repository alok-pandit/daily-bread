{
  "name": "rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "concurrently 'react-native run-android' 'pnpm prettier-watch' 'pnpm lint-watch'",
    "ios": "concurrently 'react-native run-ios' 'pnpm prettier-watch' 'pnpm lint-watch'",
    "start": "react-native start",
    "test": "jest",
    "lint": "pnpm prettier && eslint src/**/*.{js,ts,jsx,tsx} --fix",
    "lint-watch": "onchange 'src/**/*.{js,ts,jsx,tsx}' -- eslint {{changed}} --fix",
    "prettier": "prettier --write src/**/*.{js,ts,jsx,tsx}",
    "prettier-watch": "onchange 'src/**/*.{js,ts,jsx,tsx}' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "-": "0.0.1",
    "@react-native-community/cli-platform-android": "13.2.0",
    "@react-native-community/cli-platform-ios": "13.2.0",
    "@react-native/metro-config": "^0.73.5",
    "@react-navigation/native": "6.1.10",
    "@react-navigation/native-stack": "6.9.18",
    "@rnx-kit/metro-config": "1.3.14",
    "@rnx-kit/metro-resolver-symlinks": "0.1.35",
    "axios": "1.6.7",
    "clsx": "2.1.0",
    "immer": "10.0.3",
    "jotai": "2.6.4",
    "jotai-immer": "0.3.0",
    "jsc-android": "250231.0.0",
    "nativewind": "2.0.11",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gradle-plugin": "0.71.19",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.29.0",
    "tailwind-merge": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/runtime": "7.23.9",
    "@react-native-community/eslint-config": "3.2.0",
    "@tsconfig/react-native": "3.0.3",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.55",
    "@types/react-native": "0.73.0",
    "@types/react-test-renderer": "18.0.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-jest": "29.7.0",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-pii": "1.0.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-xss": "0.1.12",
    "jest": "29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "onchange": "7.1.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
